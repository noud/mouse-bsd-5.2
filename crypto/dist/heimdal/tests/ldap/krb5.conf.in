[libdefaults]
	default_realm = TEST.H5L.SE
	no-addresses = TRUE

[realms]
	TEST.H5L.SE = {
		kdc = localhost:@port@
	}

[kdc]
	database = {
		dbname = ldapi://ldap-socket:OU=KerberosPrincipals,o=test,DC=h5l,DC=se
		realm = TEST.H5L.SE
		mkey_file = @objdir@/mkey.file
	}

[logging]
	kdc = 0-/FILE:@objdir@/messages.log
	default = 0-/FILE:@objdir@/messages.log
